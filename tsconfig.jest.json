{
  "compilerOptions": {
    "target": "ES5",
    "types": ["jest", "jest-enzyme"],
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "paths": {
      "@@/*": [".dumi/tmp/*"]
    }
  },
  "exclude": ["./packages/*/esm", "./packages/*/lib"]
}
