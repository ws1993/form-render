    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "less": "^3.0.0",
    "@ant-design/icons": "^4.2.2",
    "@ant-design/plots": "^1.0.9",
    "ahooks": "^3.4.1",
    "classnames": "^2.3.1",
    "formRenderV1": "npm:form-render@^1.4.13",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {},
  "peerDependencies": {
    "antd": "4.x",
    "react": ">=16.8.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
